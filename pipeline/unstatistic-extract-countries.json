									"physicalType": "String"
								},
								"sink": {
									"name": "region",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "sub-region",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "sub-region",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
