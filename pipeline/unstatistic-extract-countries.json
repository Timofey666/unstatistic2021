									"physicalType": "String"
								},
								"sink": {
									"name": "alpha-3",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "country-code",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "country-code",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "iso_3166-2",
