{
	"name": "unstatistic-extract-countries",
	"properties": {
		"description": "Extract data from CSV into SQL",
		"activities": [
			{
				"name": "Copy countries to SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
